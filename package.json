{
  "name": "chess-stats",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "keywords": [
    "nextjs",
    "starter",
    "typescript"
  ],
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "test": "jest --maxWorkers=50%",
    "test:ci": "jest --runInBand",
