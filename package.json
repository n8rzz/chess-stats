    "@types/react": "17.0.3",
    "@types/react-dom": "17.0.3",
    "@typescript-eslint/eslint-plugin": "4.0.1",
    "@typescript-eslint/parser": "4.0.1",
    "babel-core": "6.26.3",
    "babel-jest": "26.6.3",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "jest": "26.6.3",
    "prettier": "2.2.1",
    "ts-jest": "26.5.5",
    "typescript": "4.2.4"
  }
